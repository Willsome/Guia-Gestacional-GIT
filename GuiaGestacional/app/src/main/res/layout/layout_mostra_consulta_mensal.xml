<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/fundo_branco"
    android:elevation="5dp"
    android:stateListAnimator="@null">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="10dp"
        android:orientation="vertical">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Consulta:  " />

            <TextView
                android:id="@+id/cmTvNumeroConsulta"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#4F4F4F" />

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Data da consulta:  " />

            <TextView
                android:id="@+id/cmTvDataConsulta"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#4F4F4F" />

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:layout_marginTop="10dp"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Queixa:" />

            <TextView
                android:id="@+id/cmTvQueixa"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#4F4F4F" />

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Idade Gestacional (IG):  " />

            <TextView
                android:id="@+id/cmTvIg"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#4F4F4F" />

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Peso:  " />

            <TextView
                android:id="@+id/cmTvPeso"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#4F4F4F" />

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="IMC:  " />

            <TextView
                android:id="@+id/cmTvImc"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#4F4F4F" />

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Edema:  " />

            <TextView
                android:id="@+id/cmTvEdema"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#4F4F4F" />

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Pressão Arterial (PA):  " />

            <TextView
                android:id="@+id/cmTvPa"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#4F4F4F" />

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Altura uterina:  " />

            <TextView
                android:id="@+id/cmTvAlturaUterina"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#4F4F4F" />

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Posição fetal:  " />

            <TextView
                android:id="@+id/cmTvPosicaoFetal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#4F4F4F" />

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Batimento Cardíaco Fetal (BCF):  " />

            <TextView
                android:id="@+id/cmTvBcf"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#4F4F4F" />

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Movimento fetal:  " />

            <TextView
                android:id="@+id/cmTvMovFetal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#4F4F4F" />

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:layout_marginTop="10dp"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Nome do profissional:" />

            <TextView
                android:id="@+id/cmTvNomeDoProfissional"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#4F4F4F" />

        </LinearLayout>

    </LinearLayout>

</RelativeLayout>